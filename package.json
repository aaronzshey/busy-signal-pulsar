{
  "name": "busy-signal",
  "main": "./lib/index.js",
  "version": "1.1.0",
  "description": "A package that provides an easy to use API to show your package is performing a task",
  "keywords": [
    "busy",
    "signal"
  ],
  "repository": "https://github.com/steelbrain/busy-signal",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "busy-signal": {
      "versions": {
        "1.0.0": "providerRegistry"
      }
    }
  },
  "dependencies": {
    "disposify": "^1.0.0",
    "escape-html": "^1.0.3",
    "humanize-time": "0.0.2"
  },
  "devDependencies": {
    "eslint-config-steelbrain": "^2.0.0"
  },
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "configSchema": {
    "itemsToShowInHistory": {
      "description": "Limit the number of items to show in history",
      "maximum": 10,
      "minimum": 1,
      "type": "integer",
      "default": 5
    }
  }
